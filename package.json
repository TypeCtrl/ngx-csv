{
  "name": "@ctrl/ngx-csv",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "prebuild": "rimraf dist",
    "build": "ts-node build.ts",
    "publish": "node prepublish.js && cd dist && npm publish --access public",
    "test": "ng test --browsers=ChromeCI",
    "test:ci": "ng test --single-run --code-coverage --no-progress --browsers=ChromeCI",
    "lint": "ng lint --type-check -t stylish",
    "lint:fix": "ng lint --fix --type-check -t stylish",
    "bundlesize": "npx bundlesize",
    "commitlint": "commitlint -e $GIT_PARAMS",
    "travis-deploy-once": "travis-deploy-once",
    "commit": "git-cz",
    "semantic-release": "cd dist && semantic-release",
    "ghpages": "ng build --prod --no-progress --baseHref='/ngx-csv/'"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "6.0.0",
    "@angular/common": "6.0.0",
    "@angular/compiler": "6.0.0",
    "@angular/core": "6.0.0",
    "@angular/forms": "6.0.0",
    "@angular/http": "6.0.0",
    "@angular/platform-browser": "6.0.0",
    "@angular/platform-browser-dynamic": "6.0.0",
    "@angular/router": "6.0.0",
    "@ctrl/ngx-github-buttons": "2.0.2",
    "bootstrap": "4.1.1",
    "core-js": "2.5.5",
    "rxjs": "6.1.0",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular/cli": "6.0.0",
    "@angular/compiler-cli": "6.0.0",
    "@angular/language-service": "6.0.0",
    "@types/fs-extra": "5.0.2",
    "@types/jasmine": "2.8.7",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "9.6.12",
    "codelyzer": "4.3.0",
    "commitizen": "2.9.6",
    "cz-conventional-changelog": "2.1.0",
    "fs-extra": "6.0.0",
    "jasmine-core": "2.99.1",
    "karma": "2.0.2",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "1.4.2",
    "karma-jasmine": "1.1.2",
    "karma-jasmine-html-reporter": "0.2.2",
    "karma-mocha-reporter": "2.2.5",
    "ng-packagr": "3.0.0-rc.3",
    "rimraf": "2.6.2",
    "semantic-release": "15.2.0",
    "travis-deploy-once": "5.0.0",
    "ts-node": "6.0.3",
    "tslint": "5.10.0",
    "typescript": "2.7.2",
    "@angular-devkit/build-angular": "0.6.0"
  },
  "release": {
    "branch": "master"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "bundlesize": [
    {
      "path": "./dist/bundles/ctrl-ngx-csv.umd.min.js",
      "maxSize": "2 kB"
    }
  ]
}
